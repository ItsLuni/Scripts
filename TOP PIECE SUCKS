cal Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TOP PIECE SUCKS", "Ocean")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Get All Legend Fruits")
Section:NewToggle("ON-OFF", ".", function(state)
    if state then
        _G.Fruit = true -- Spins for fruits
_G.Drop = true -- Drops all fruits / tools in inv
           
loadstring(game:HttpGet("https://roflux.net/scripts/games/open/rainbowpiece.lua", true))()
    else
        _G.Fruit = false -- Spins for fruits
_G.Drop = true -- Drops all fruits / tools in inv
           
loadstring(game:HttpGet("https://roflux.net/scripts/games/open/rainbowpiece.lua", true))()
    end
end)
